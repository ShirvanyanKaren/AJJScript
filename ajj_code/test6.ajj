class A {
    int a;
    int b;
    init(int a, int b) {
        this.a = a;
        this.b = b;
    }

    method print() void{
        println(this.a);
        println(this.b);
    }

}

class B extends A {
    int c;
    super(a, b, c) {
        this.c = c;
    }


}


B a = new A(1, 2);
a.print();